{% extends 'dashboard.html.twig' %}

{% block title %} {{ 'company.adding'|trans }} {% endblock %}

{% block body %}
    {{ form_start(form) }}
    <div class="form-actions">
        <div class="col-sm-12">
            {{ form_widget(form.submit, { 'attr': {'class': 'btn btn-primary'}}) }}
            <a class="btn btn-default" href="{{ path('company') }}">
                {{ 'backToTheList'|trans }}
            </a>
        </div>
    </div>

    {{ form_rest(form) }}
    {{ form_end(form) }}
{% endblock %}
