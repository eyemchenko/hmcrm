{
    "name": "andreybolonin/hmcrm",
    "license": "proprietary",
    "type": "project",
    "description": "All-in-one sales platform",
    "keywords": ["crm","hmcrm","sales", "marketing"],
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "3.1.*",
        "doctrine/orm": "2.5.*",
        "doctrine/doctrine-bundle": "~1.6",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.8",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "doctrine/doctrine-fixtures-bundle": "^2.2",
        "knplabs/knp-menu-bundle": "^2.0",
        "symfony/security-acl": "^3.0",
        "fungio/dataexporter-bundle": "dev-master",
        "asana/asana": "^0.4.1",
        "jeroendesloovere/wunderlist-php-api": "^1.0",
        "cdaguerre/php-trello-api": "^0.9.3",
        "minneola/todoist": "dev-master",
        "mailerlite/mailerlite-api-v2-php-sdk": "^0.1.5",
        "getresponse/getresponse": "^0.0.2",
        "mailchimp/mailchimp": "^2.0",
        "aweber/aweber": "^1.1",
        "constantcontact/constantcontact": "3.0.0-alpha",
        "sunra/turbosms-bundle": "^0.1.0",
        "zelenin/smsru": "^4.1",
        "vresh/twilio-bundle": "^1.0",
        "arcturial/clickatell": "^2.1",
        "plivo/plivo-php": "^1.1",
        "connect-corp/nexmo-client": "dev-master",
        "camcima/tropo-webapi-php": "dev-master",
        "archer/clickatell-bundle": "^0.1.0",
        "markroland/ontraport-api-php-class": "^0.3.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
